datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  user_id  String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model Email {
  email_id String @id @default(uuid())
  email    String
  team     String
}
